  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npx next build",
    "export": "npx next export -o dist",
    "postexport": "next-sitemap",
    "deploy": "firebase deploy --only hosting",
    "deploy:functions": "cd ./functions && npm run deploy",
    "lint": "eslint ./src/ --ext ts,js,tsx,jsx",
    "lint:fix": "eslint ./src/ --ext ts,js,tsx,jsx --fix",
    "start": "next start",
    "format": "prettier 'src/**/*.{js,jsx,ts,tsx,json,css}' --write",
    "type-check": "tsc",
    "install:initial": "npm install && cd functions && npm install && cp ../.env.local.example ../.env.local && cd ..",
    "test": "npx jest",
    "unit-test": "npx jest --testMatch \"**/*.unit.test.*\"",
    "int-test": "npx jest --testMatch \"**/*.int.test.*\""
  },
  "browserslist": [
    "> 1%"
  ],
  "husky": {
